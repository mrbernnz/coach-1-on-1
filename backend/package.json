{
  "name": "backend",
  "version": "0.0.0",
  "description": "backend service for 1-on-1 Coaching Calls",
  "main": "app.ts",
  "author": "Noel Sagaille",
  "license": "none",
  "private": true,
  "scripts": {
    "start:dev": "nodemon",
    "db:create": "ts-node-esm ./node_modules/typeorm-extension/bin/cli.mjs db:create -d src/configs/dataSource.ts",
    "db:drop": "ts-node ./node_modules/typeorm-extension/bin/cli.mjs db:drop -d src/configs/dataSource.ts",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d src/configs/dataSource.ts",
    "db:migrate:revert": "typeorm-ts-node-commonjs migration:revert -d src/configs/dataSource.ts",
    "db:seed": "ts-node-esm node_modules/typeorm-extension/bin/cli.mjs seed:run -d src/configs/dataSource.ts",
    "db:seed:create": "ts-node-esm node_modules/typeorm-extension/bin/cli.mjs seed:create",
    "migration:create": "typeorm-ts-node-esm migration:create"
  },
  "dependencies": {
    "@hapi/boom": "10.0.1",
    "@koa/bodyparser": "5.1.0",
    "@koa/cors": "5.0.0",
    "@koa/router": "12.0.1",
    "koa": "2.15.2",
    "koa-compose": "4.1.0",
    "koa-pino-logger": "4.0.0",
    "moment": "2.30.1",
    "pg": "8.11.3",
    "pino": "8.19.0",
    "pino-pretty": "11.0.0",
    "rambda": "9.1.1",
    "react-big-calendar": "1.11.2",
    "react-big-schedule": "4.4.0",
    "typeorm": "0.3.20",
    "typeorm-extension": "3.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/koa": "2.15.0",
    "@types/koa-bodyparser": "4.3.12",
    "@types/koa-compose": "3.2.8",
    "@types/koa__cors": "5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "5.0.0",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3"
  }
}
